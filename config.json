{
    "cloud": "aws",
    "telemetry_topic": "telemetry/rpi-1",
    "control_topic": "control/rpi-1",
    "providers": [{
        "type": "aws",
        "client_id": "rpi-1",
        "thing_id": "iot-demo-rpi-1",
        "endpoint": "a1bqyju1450wf9-ats.iot.eu-west-1.amazonaws.com",
        "port": 8883,
        "root": "./certs/root-ca.pem",
        "cert": "./certs/8098cdfe96-certificate.pem.crt",
        "private": "./certs/8098cdfe96-private.pem.key",
        "mode": "STAND"
    }],
    "use-case": "Standard",
    "use-cases": [{
        "use-case": "Engineering",
        "push": 10,
        "payload-type": "JSON",
        "devices": ["Sensor", "CPU", "Memory", "Green", "Yellow", "Red"]
    }, {
        "use-case": "Standard",
        "push": 30,
        "payload-type": "PROTOBUF",
        "devices": ["Sensor", "CPU"]
    }, {
        "use-case": "Shadow",
        "push": 30,
        "payload-type": "JSON",
        "devices": ["Sensor", "CPU", "Green", "Yellow", "Red"]
    }],
    "devices": [{
        "type": "led",
        "name": "Green",
        "pin": 26
    }, {
        "type": "led",
        "name": "Yellow",
        "pin": 19
    }, {
        "type": "led",
        "name": "Red",
        "pin": 13
    }, {
        "type": "dht11",
        "name": "Sensor",
        "pin": 4
    }, {
        "type": "cputemp",
        "name": "CPU",
        "location": "/sys/class/thermal/thermal_zone0/temp"
    }, {
        "type": "cpuprec",
        "name": "CPU"
    }, {
        "type": "memory",
        "name": "Memory"
    }],
    "logging": {
        "level": "DEBUG",
        "error-file": "./logs/error.log"
    }
}